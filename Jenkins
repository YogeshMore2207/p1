pipeline {
    agent any
    tools {
        nodejs 'nodejs'
    }
    stages {
        stage('Build') {
            steps {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'Github', url: 'https://github.com/YogeshMore2207/react-app.git']])
                sh 'npm install'
                sh 'npm run build'
                sh 'npm start'
            }
        }
       }
}
